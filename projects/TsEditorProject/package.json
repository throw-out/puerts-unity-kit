        "runtime:publish-esm": "node ./scripts/runtime.mjs --esm --publish",
        "---------------TOOLS----------------": "",
        "tools:link.xml": "node ./scripts/tools.mjs --method link.xml --dev --watch",
        "tools:publish:link.xml": "node ./scripts/tools.mjs --method link.xml --publish",
        "---------------TESTS----------------": "",
        "tests:link.xml": "node ./tests/link.xml.js"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^18.7.23",
        "buffer": "^6.0.3",
        "eslint": "^7.32.0",
        "fast-glob": "^3.3.2",
        "minimist": "^1.2.7",
        "source-map-support": "^0.5.19",
        "ts-loader": "^9.4.2",
        "ts-morph": "^24.0.0",
        "typescript": "^5.3.3",
        "webpack": "^5.90.3",
        "webpack-cli": "^5.1.4"
    }
